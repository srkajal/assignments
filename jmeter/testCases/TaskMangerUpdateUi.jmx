        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="Task WebDriver Sampler" enabled="true">
          <stringProp name="WebDriverSampler.script">var pkg = JavaImporter(org.openqa.selenium); //WebDriver classes
var support_ui = JavaImporter(org.openqa.selenium.support.ui.WebDriverWait); //WebDriver classes
var wait = new support_ui.WebDriverWait(WDS.browser, 5000);

WDS.sampleResult.sampleStart(); //captures sampler&apos;s start time
WDS.sampleResult.getLatency();
WDS.log.info(&quot;Sample started&quot;);

WDS.browser.get(&apos;http://localhost:8080/task-manager/&apos;); //opens website specified in &apos;http://duckduckgo.com&apos;
WDS.log.info(&quot;Sample ended - navigated to Task Manger&quot;);

WDS.browser.findElement(pkg.By.cssSelector(&quot;div:nth-child(${editTask}) .col-md-1:nth-child(6) .btn&quot;)).click();

// 6 | click | id=taskName |  | 
WDS.browser.findElement(pkg.By.id(&quot;taskName&quot;)).click();
// 7 | type | id=taskName | Selenium Tetsing | 
WDS.browser.findElement(pkg.By.id(&quot;taskName&quot;)).sendKeys(&quot;Task-${editTask}&quot;);
// 22 | click | css=.col-md-2:nth-child(2) &gt; .btn |  | 
WDS.browser.findElement(pkg.By.cssSelector(&quot;.col-md-2:nth-child(2) &gt; .btn&quot;)).click();

java.lang.Thread.sleep(200);
WDS.log.info(&quot;Task completed&quot;);
WDS.sampleResult.sampleEnd();</stringProp>
